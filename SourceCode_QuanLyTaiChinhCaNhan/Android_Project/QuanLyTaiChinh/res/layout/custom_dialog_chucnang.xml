<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical"
    android:padding="3dp" >

     <ImageView
        android:id="@+id/imgAnhChuNang_CD_CN"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:contentDescription="@string/anhnhom"
        android:src="@drawable/ic_khonganh" />

    <TextView
        android:id="@+id/tvTenChucNang_CD_CN"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:gravity="center_vertical"
        android:paddingLeft="20dp"
        android:paddingRight="5dp"
        android:textSize="15sp"
        android:textStyle="normal"
        android:textColor="@color/mauden"
        android:layout_toRightOf="@+id/imgAnhChuNang_CD_CN"/>
</RelativeLayout>